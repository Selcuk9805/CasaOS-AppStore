	"container": {
		"image": "technitium/dns-server:latest",
		"shell": "sh",
		"privileged": false,
		"network_model": "bridge",
		"web_ui": {
			"http": "5380",
			"path": ""
		},
		"health_check": "",
		"envs": [
			{
				"key": "DNS_SERVER_DOMAIN",
				"value": "dns-server",
				"description": "",
				"configurable": "no"
			}
		],
		"ports": [
			{
				"container": "5380",
				"host": "5380",
				"type": "tcp",
				"allocation": "automatic",
				"configurable": "no",
				"description": "Technitium HTTP Port"
			},

			{
				"container": "53",
				"host": "53",
				"type": "tcp",
				"allocation": "automatic",
				"configurable": "no",
				"description": "Technitium DNS Port"
			},

			{
				"container": "53",
				"host": "53",
				"type": "udp",
				"allocation": "automatic",
				"configurable": "no",
				"description": "Technitium DNS Port"
			}
		],
		"volumes": [
			{
				"container": "/etc/dns",
				"host": "/DATA/AppData/technitium-dns-server",
				"mode": "rw",
				"allocation": "automatic",
				"configurable": "no",
				"description": "dns server directory"
			}
		],
